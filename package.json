{
  "name": "@stratodem/js-archetype",
  "version": "1.6.0",
  "description": "The StratoDem Analytics scaffolding for a JavaScript project",
  "license": "MIT",
  "dependencies": {
    "babel-eslint": "^8.0.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "flow-bin": "^0.59.0",
    "npm-check-updates": "^2.13.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-test-renderer": "^16.1.1"
  },
  "devDependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StratoDem/js-archetype.git"
  },
  "jest": {
    "modulePaths": [
      "<rootDir>/js"
    ],
    "testResultsProcessor": "./node_modules/jest-junit",
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "^react-table/react-table.css$": "identity-obj-proxy",
      "^react-bootstrap-slider/src/css/bootstrap-slider.min.css$": "identity-obj-proxy",
      "^config$": "<rootDir>/configs/app-config.js",
      "\\.(css|less)$": "identity-obj-proxy",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "testRegex": "\\.test\\.js$",
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>assets/js/_test-env.js"
    ],
    "collectCoverage": true,
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "jest-junit": {
    "suiteName": "ArgosUnchained jest tests",
    "output": "./junit.xml",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "usePathForSuiteName": "true"
  },
  "keywords": [
    "django",
    "real-estate",
    "estate",
    "what-if"
  ],
  "author": "StratoDem Analytics",
  "bugs": {
    "url": "https://github.com/StratoDem/ArgosUnchained/issues"
  },
  "homepage": "https://github.com/StratoDem/ArgosUnchained#readme",
  "scripts": {
    "test": "jest --verbose --coverage || true",
    "test_update": "jest --verbose -u",
    "check_updates": "node_modules/.bin/npm-check-updates"
  }
}
